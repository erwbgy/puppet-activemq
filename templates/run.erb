#!/bin/bash
exec 2>&1
export ACTIVEMQ_HOME=<%= @product_dir %>
export ACTIVEMQ_BASE=$ACTIVEMQ_HOME
mkdir -p $ACTIVEMQ_BASE/temp
export ACTIVEMQ_OPTS="-Xms<%= @min_mem %> -Xmx<%= @max_mem %> -Djava.io.tmpdir=$ACTIVEMQ_BASE/temp <%= @java_opts %>"
export JAVA_HOME=<%= @java_home %>
cd <%= @product_dir %>
exec $ACTIVEMQ_HOME/bin/activemq console <%= @config_file %>
